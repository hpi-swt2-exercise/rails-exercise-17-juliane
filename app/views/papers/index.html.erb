<h1>Listing Papers</h1>

<%= link_to 'Add paper', new_paper_path %>

<table>
  <tr>
    <th>Title</th>
    <th>Venue</th>
    <th>Year</th>
  </tr>

  <% @papers.each do |paper| %>
      <tr>
        <td><%= paper.title %></td>
        <td><%= paper.venue %></td>
        <td><%= paper.year %></td>
        <td><%= link_to 'Show', paper_path(paper.id) %></td>
        <td><%= link_to 'Edit', edit_paper_path(paper.id) %></td>
  <% end %>
</table>
</table>